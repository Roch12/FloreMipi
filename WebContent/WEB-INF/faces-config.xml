<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<message-bundle>resources.application</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
	</application>
	<managed-bean>
		<managed-bean-name>ClientBean</managed-bean-name>
		<managed-bean-class>com.floremipi.model.ClientBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>clientList</property-name>
			<property-class>java.util.ArrayList</property-class>
			<list-entries></list-entries>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<display-name>Client/clientList.xhtml</display-name>
		<from-view-id>/Client/clientList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>clientEdit</from-outcome>
			<to-view-id>/Client/clientEdit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>clientDel</from-outcome>
			<to-view-id>/Client/clientList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Client/clientEdit.xhtml</display-name>
		<from-view-id>/Client/clientEdit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>clientList</from-outcome>
			<to-view-id>/Client/clientList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>clientDel</from-outcome>
			<to-view-id>Client/clientEdit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Stock/stockList.xhtml</display-name>
		<from-view-id>/Stock/stockList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sotckEdit</from-outcome>
			<to-view-id>/Stock/stockEdit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>stockDel</from-outcome>
			<to-view-id>Client/stockEdit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Stock/stockEdit.xhtml</display-name>
		<from-view-id>/Stock/stockEdit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sotckList</from-outcome>
			<to-view-id>/Stock/stockEdit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>stockDel</from-outcome>
			<to-view-id>Client/stockEdit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
